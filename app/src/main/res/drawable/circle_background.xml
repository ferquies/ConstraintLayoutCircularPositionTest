<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="#00000000"/>
            <padding
                android:right="@dimen/circle_background_padding"
                android:left="@dimen/circle_background_padding"
                android:top="@dimen/circle_background_padding"
                android:bottom="@dimen/circle_background_padding"/>
